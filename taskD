#include <iostream>
#include <vector>
using std::cin;
using std::cout;
using std::swap;
using std::ios_base;

void kth_num(std::vector<int>& num, int K) {
    int left = 0, right = num.size() - 1;
    int x;
    while(left != right){
        x = num[(rand() % (right - left)) + left];
        int i = left, j = right;
        while(i < j) {
            while(i < right && num[i] < x)++i;
            while(j > left && num[j] > x)--j;
            if(i < j) {
                swap(num[i], num[j]);
                ++i;
                --j;
            }
        }
        while(i < right && num[i] < x)++i;
        while(j > left && num[j] > x)--j;
        if(K == i && i == j){
            left = right;
        }else{
            if(K < i) {
                right = i - 1;
            }else{
                left = j + 1;
            }
        }
    }
}

int main() {
    ios_base::sync_with_stdio(false);
    int N, K;
    cin >> N >> K;
    std::vector<int> num(N);
    for(int i = 0; i <  N; ++i) {
        cin >> num[i];
    }
    kth_num(num, K);
    cout << num[K] << "\n";
    return 0;
}
