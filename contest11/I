#include <iostream>
#include <vector>

std::vector<int> getZFunction(const std::string& str) {
    if (str.empty())
        return {0};
    int zBiggest = 0;
    int zBorder = -1;
    std::vector<int> z(1, 0);
    for (int i = 1; i < str.size(); ++i) {
        if (i <= zBorder && i + z[i - zBiggest] - 1 < zBorder)
            z.push_back(z[i - zBiggest]);
        else {
            zBiggest = i;
            zBorder = std::max(i - 1, zBorder);
            while (zBorder < str.size() && str[zBorder + 1] == str[zBorder - i + 1])
                ++zBorder;
            z.push_back(zBorder - i + 1);
        }
    }
    return z;
}

int main() {
    std::string str;
    std::cin >> str;
    std::vector<int> z = getZFunction(str);
    for (auto i:z) std::cout << i << ' ';
    return 0;
}
