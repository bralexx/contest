#include <iostream>
#include <vector>

using std::cout;
using std::string;
using std::cin;
template<class T>
struct Stack {
    T* buffer = new T(1);
    int size = 0;
    int array_size = 1;

    Stack() {}
    Stack(const Stack&) = delete;
    bool operator= (const Stack&) = delete;
    ~Stack() {}

    int getsize()const {
        return size;
    }

    void push(const T& x) {
        if(array_size == size){
            T* new_buffer = new T[array_size * 2];
            for(int i = 0; i < size; ++i) {
                new_buffer[i] = buffer[i];
            }
            delete[] buffer;
            buffer = new_buffer;
            array_size *= 2;
        }
        buffer[size] = x;
        ++size;
    }


    T pop() {
        T last_buffer = buffer[size - 1];
        if(array_size / 4 > size) {
            T* new_buffer = new T[array_size / 2];
            for(int i = 0; i < size; ++i) {
                new_buffer[i] = buffer[i];
            }
            delete[] buffer;
            buffer = new_buffer;
            array_size /= 2;
        }
        --size;
        return last_buffer;
    }


    T back()const {
        return buffer[size - 1];
    }


    void clear() {
        delete[] buffer;
        buffer = new T[1];
        array_size = 1;
        size = 0;
    }


    bool empty()const {
        return size == 0;
    }
};


int main() {
    Stack<int> st;
    Stack<char> st2;
    st2.push('f');
    st2.push('g');
    st2.pop();
    string in;
    do {
        cin >> in;

        if(in == "push") {
            int x;
            cin >> x;
            st.push(x);
            cout << "ok\n";
        }

        if( in == "pop" ) {
            if(!st.empty()) {
                cout << st.pop() << "\n";
            }else{
                cout << "error\n";
            }
        }

        if( in == "back" ){
            if(!st.empty()) {
                cout << st.back() << "\n";
            }else{
                cout << "error\n";
            }
        }

        if( in == "size" ) {
            cout << st.getsize() << "\n";
        }

        if(in == "clear") {
            st.clear();
            cout << "ok\n";
        }
    }while(in != "exit");
    cout << "bye";
}
