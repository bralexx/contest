#include <iostream>
#include <vector>

using std::cout;
using std::string;
using std::cin;
template<class T>
struct Stack {
    T* N = new T[1];
    int size = 0;
    int allsize = 1;

    Stack() {
    }

    ~Stack() {
    }

    int getsize()const {
        return size;
    }

    push(const T& x) {
        if(allsize == size){
            T* NN = new T[allsize * 2];
            for(int i = 0; i < size; ++i) {
                NN[i] = N[i];
            }
            delete[] N;
            N = NN;
        allsize *= 2;
        }
        N[size] = x;
        ++size;
    }


    T pop() {
        T lastN = N[size-1];
        if(allsize/4 > size) {
            T* NN = new T[allsize/2];
            for(int i = 0; i < size; ++i){
                NN[i] = N[i];
            }
            delete[] N;
            N = NN;
        allsize /= 2;
        }
        --size;
        return lastN;
    }


    T back()const {
        return N[size-1];
    }


    clearall() {
        N = new T[1];
        allsize = 1;
        size = 0;
    }


    bool empty()const {
        return (size == 0 ? true : false);
    }
};


int main() {
    Stack<int> st;
    string in;
    do {
        cin >> in;

        if(in == "push") {
            int x;
            cin >> x;
            st.push(x);
            cout << "ok\n";
        }

        if( in == "pop" ) {
            if(!st.empty()) {
                cout << st.pop() << "\n";
            }else{
                cout << "error\n";
            }
        }

        if( in == "back" ){
            if(!st.empty()) {
                cout << st.back() << "\n";
            }else{
                cout << "error\n";
            }
        }

        if( in == "size" ) {
                cout << st.getsize() << "\n";
        }

        if(in == "clear") {
                st.clearall();
                cout << "ok\n";
            }
    }while(in != "exit");
    cout << "bye";
}
